{
  "name": "unistudproj-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "build": "rm -rf dist/ && tsc && tsc-alias",
    "start": "node dist/src/server.js",
    "tsc:watch": "tsc-watch --onSuccess \"tsc-alias\"",
    "start:dev": "concurrently \"yarn tsc:watch\" \"nodemon dist/src/server.js\"",
    "start:debug": "concurrently \"yarn tsc:watch\" \"nodemon --inspect dist/src/server.js\"",
    "start:prod": "SET NODE_ENV=PRODUCTION && yarn dev",
    "test": "SET NODE_ENV=PRODUCTION && nyc --reporter=text-summary mocha --inspect",
    "test:watch": "concurrently \"yarn tsc:watch\" \"nodemon -x yarn test\"",
    "test:coverage": "c8 --reporter=text-summary mocha --exit",
    "lint": "eslint --cache src/",
    "lint:fix": "eslint --fix src/",
    "format": "prettier --write src/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.12.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "lint-staged": "^13.2.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "tsc-alias": "^1.8.5",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.3"
  }
}
