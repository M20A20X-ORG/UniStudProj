{
  "name": "unistudproj-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "build": "rm -rf dist/ && tsc && tsc-alias",
    "start": "node dist/index.js",
    "start:debug": "node --inspect dist/index.js",
    "watch": "tsc-watch --onSuccess \"tsc-alias\"",
    "nodemon": "nodemon -x \"yarn start\"",
    "nodemon:debug": "nodemon -x \"yarn start:debug\"",
    "dev": "concurrently \"yarn watch\" \"yarn nodemon\"",
    "dev:debug": "concurrently \"yarn watch\" \"yarn nodemon:debug\"",
    "prod": "SET NODE_ENV=PRODUCTION && yarn dev",
    "lint": "eslint --cache src/",
    "lint:fix": "eslint --fix src/",
    "format": "prettier --write src/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.12.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "tsc-alias": "^1.8.5",
    "typescript": "^5.0.3"
  }
}
